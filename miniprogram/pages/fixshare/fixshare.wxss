/* pages/fixshare/fixshare.wxss */
.page {
  background-color: #FFFACD;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.blank {
  display: inline-block;
  margin-top: 50rpx;
  margin-bottom: 50rpx;
}
.row {
  width: 90%;
  margin-top: 30rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
}
.item{
  margin-left: 25rpx;
  margin-top: 25rpx;
  margin-right: 25rpx;
  margin-bottom: 25rpx;
  width: 150rpx;
  height: 150rpx;
  display: inline-block;
}
.nameRow {
  width: 90%;
  margin-top: 20rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
}
.name{
  margin-left: 25rpx;
  margin-right: 25rpx;
  width: 150rpx;
  height: 30rpx;
  line-height: 30rpx;
  text-align: center;
  color: #8B8989;
  font-size: 12pt;
}

