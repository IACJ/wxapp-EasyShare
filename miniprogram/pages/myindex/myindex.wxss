/* pages/myindex/myindex.wxss */

.page {
  background-color: #EEE9E9;
  padding: 50rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.topArea{
  margin-top: 50rpx;
  width: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-around;
}

.btnScan{
  width: 100%;
  height: 150rpx;
  margin-bottom: 10rpx;
  text-align: center;
  line-height: 150rpx;
  font-size: 16pt;
  color: white;
  background-color: #CDC9C9;
}

.chooseArea{
  width: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  margin-top: 50rpx;
}

.icon-move{
  background-image: url("data:image/png;base64,");
  background-size: cover;
}

.icon-fix{
  background-image: url("data:image/png;base64,");
  background-size: cover;
}

.btnBlock {
  width: 100rpx;
  height: 100rpx;
  display: inline-block;
}

.btnRoom{
  width: 280rpx;
  height: 150rpx;
  padding-left: 30rpx;
  background-color: white;
  display: flex;
  flex-direction: row;
  align-items: center;
}
